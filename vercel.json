{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "outputDirectory": "dist",
  "functions": {
    "api/cron/refresh-observations.js": {
      "maxDuration": 60
    },
    "api/cron/refresh-observations-ca.js": {
      "maxDuration": 60
    },
    "api/cron/refresh-observations-or.js": {
      "maxDuration": 60
    },
    "api/cron/refresh-observations-wa.js": {
      "maxDuration": 60
    },
    "api/trails/[state].js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/refresh-observations-ca",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/refresh-observations-or",
      "schedule": "2 6 * * *"
    },
    {
      "path": "/api/cron/refresh-observations-wa",
      "schedule": "4 6 * * *"
    }
  ]
}